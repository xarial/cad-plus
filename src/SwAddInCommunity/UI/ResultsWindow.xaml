<Window x:Class="Xarial.CadPlus.AddIn.Sw.UI.ResultsWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:sharedui="clr-namespace:Xarial.CadPlus.Plus.Shared.UI;assembly=Xarial.CadPlusPlus.Shared"
             WindowStartupLocation="CenterOwner" WindowStyle="ToolWindow"
             mc:Ignorable="d" 
             Height="450" Width="600">
    <TabControl>
        <TabItem Header="Summary">
            <sharedui:JobSummaryControl/>
        </TabItem>
        <TabItem Header="Status">
            <sharedui:JobReportControl/>
        </TabItem>
        <TabItem Header="Journal">
            <sharedui:JobJournalControl/>
        </TabItem>
    </TabControl>
</Window>
